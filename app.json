{
  "expo": {
    "name": "Roomies",
    "slug": "roomies",
    "scheme": "roomies",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./logo.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "assetBundlePatterns": [
      "assets/**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vibecode.app",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "UIBackgroundModes": ["fetch", "remote-notification"]
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "softwareKeyboardLayoutMode": "resize",
      "package": "com.vibecode.app",
      "googleServicesFile": "./google-services.json"
    },
    "plugins": [
      "expo-asset",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "./plugins/withModularHeaders.js",
      "./plugins/withReactCommonHeaders.js",
      "expo-dev-client",
      "expo-font",
      "expo-mail-composer",
      [
        "expo-notifications",
        {
          "icon": "./logo.png",
          "color": "#ffffff",
          "mode": "production"
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/messaging",
      "expo-secure-store",
      "expo-sqlite",
      "expo-video",
      "expo-web-browser",
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-4539954746841772~6975731872",
          "iosAppId": "ca-app-pub-4539954746841772~9470743711"
        }
      ]
    ]
  }
}
